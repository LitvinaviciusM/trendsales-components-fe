{
	"version": "0.0.1",
	"license": "MIT",
	"main": "dist/index.js",
	"name": "trendsales-components-fe",
	"author": "litvinaviciusm@gmail.com",
	"module": "dist/trendsales-components-fe.esm.js",
	"typings": "dist/index.d.ts",
	"engines": {
		"node": ">=10"
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"start": "tsdx watch",
		"build": "tsdx build --format esm --tsconfig tsconfig.json",
		"test": "tsdx test --passWithNoTests",
		"lint": "tsdx lint",
		"prepare": "tsdx build",
		"format": "prettier --config .prettierrc --write 'src/**/*.tsx'",
		"size": "size-limit",
		"analyze": "size-limit --why",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"husky": {
		"hooks": {
			"pre-commit": "tsdx lint"
		}
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
			"\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
		}
	},
	"size-limit": [
		{
			"path": "dist/trendsales-components-fe.cjs.production.min.js",
			"limit": "1 KB"
		},
		{
			"path": "dist/trendsales-components-fe.esm.js",
			"limit": "1 KB"
		}
	],
	"peerDependencies": {
		"react": ">=16"
	},
	"devDependencies": {
		"@babel/core": "^7.18.2",
		"@babel/preset-env": "^7.18.2",
		"@babel/preset-react": "^7.17.12",
		"@size-limit/preset-small-lib": "^7.0.8",
		"@storybook/addon-a11y": "^6.5.6",
		"@storybook/addon-essentials": "^6.5.6",
		"@storybook/addon-info": "^5.3.21",
		"@storybook/addon-interactions": "^6.5.6",
		"@storybook/addon-links": "^6.5.6",
		"@storybook/addons": "^6.5.6",
		"@storybook/builder-webpack5": "^6.5.6",
		"@storybook/manager-webpack5": "^6.5.6",
		"@storybook/react": "^6.5.6",
		"@storybook/testing-library": "0.0.11",
		"@testing-library/react": "^13.3.0",
		"@types/react": "^18.0.9",
		"@types/react-dom": "^18.0.5",
		"auto": "^10.37.1",
		"babel-loader": "^8.2.5",
		"chromatic": "^6.5.4",
		"husky": "^8.0.1",
		"prettier": "^2.6.2",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"react-is": "^18.1.0",
		"size-limit": "^7.0.8",
		"tsdx": "^0.14.1",
		"tslib": "^2.4.0",
		"typescript": "^4.7.2"
	},
	"dependencies": {},
	"repository": "git@github.com:LitvinaviciusM/trendsales-components-fe.git"
}
